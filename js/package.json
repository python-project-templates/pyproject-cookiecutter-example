
{
  "name": "pyproject_cookiecutter_example",
  "private": true,
  "version": "0.1.0",
  "description": "An example instance of [python-project-templates/pyproject-cookiecutter](https://github.com/python-project-templates/pyproject-cookiecutter), for testing",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "widgets"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js",
    "css/*.css"
  ],
  "homepage": "https://github.com/python-project-templates/pyproject-cookiecutter",
  "bugs": {
    "url": "https://github.com/python-project-templates/pyproject-cookiecutter/issues"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Tim Paine",
    "email": "t.paine154@gmail.com"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/python-project-templates/pyproject-cookiecutter"
  },
  "scripts": {
    "build:babel": "babel src/ --source-maps --out-dir lib/",
    "build:labextension": "jupyter labextension build .",
    "build:nbextension": "webpack --mode=production",
    "build": "npm-run-all clean build:*",
    "clean": "npm-run-all clean:*",
    "clean:lib": "rimraf lib",
    "clean:labextension": "rimraf ../pyproject_cookiecutter_example/labextension",
    "clean:nbextension": "rimraf ../pyproject_cookiecutter_example/nbextension/static/index.js ../pyproject_cookiecutter_example/nbextension/static/index.js.map",
    "fix": "yarn lint --fix",
    "lint": "eslint -c .eslintrc.js --ext .js src/ tests/",
    "prepack": "npm run build",
    "test": "jest --coverage --collectCoverageFrom=src/*.{js}",
    "watch": "npm-run-all -p watch:*",
    "watch:lib": "yarn build:lib --watch",
    "watch:nbextension": "webpack --watch"
  },
  "jupyterlab": {
    "extension": "lib/plugin.js",
    "outputDir": "../pyproject_cookiecutter_example/labextension",
    "discovery": {
      "server": {
        "base": {
          "name": "pyproject_cookiecutter_example"
        },
        "managers": [
          "pip"
        ]
      }
    }
  },
  "dependencies": {
    "@jupyter-widgets/base": "^6.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@jupyterlab/builder": "^3.5.2",
    "@lumino/application": "^1.31.1",
    "@lumino/widgets": "^1.34.0",
    "babel-jest": "^29.0.3",
    "css-loader": "^6.7.3",
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "expect.js": "^0.3.1",
    "fs-extra": "^11.1.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "jest-junit": "^15.0.0",
    "jest-transform-css": "^6.0.1",
    "mkdirp": "^2.1.2",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.0",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.2.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^5.0.1"
  }
}
